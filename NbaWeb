    <script>
        // Basit bir JavaScript fonksiyonu
        document.addEventListener('DOMContentLoaded', function() {
            // Arama kutusu işlevselliği
            const searchBoxes = document.querySelectorAll('.search-box input');
            const searchButtons = document.querySelectorAll('.search-box button');
            
            searchButtons.forEach((button, index) => {
                button.addEventListener('click', function() {
                    const searchTerm = searchBoxes[index].value;
                    if(searchTerm.trim() !== '') {
                        alert('Arama: ' + searchTerm + '\n(Gerçek bir uygulamada, burada arama sonuçları gösterilecektir.)');
                    }
                });
            });

            // Linklere tıklama işlevselliği
            const navLinks = document.querySelectorAll('.nav-links a');
            navLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    navLinks.forEach(l => l.classList.remove('active'));
                    this.classList.add('active');
                    alert('Bu sayfaya geçiş: ' + this.textContent + '\n(Gerçek bir uygulamada, ilgili sayfa yüklenecektir.)');
                });
            });
        });
    </script>
